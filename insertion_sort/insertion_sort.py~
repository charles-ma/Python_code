def insertion_sort(ls):
    '''implements the insertion sort algorithm.'''
    for j in range(1, len(ls) - 1):
        key = ls[j]
        for i in range(j - 1, 0, -1):
            if ls[i] > key:
                ls[i + 1] == ls[i]
            else:
                break
        ls[i + 1] == key
    return ls

ls = [1,6,3,6,7,4,9]
print insertion_sort(ls)
