class super_class_1(object):
    def __init__(self):
        self.name = '1'

    def set_name(self, string):
        self.name = string

class super_class_2(object):
    def __init__(self):
        self.name = '2'
    
    def set_name(self):
        self.name = '3'

class derived_class(super_class_1, super_class_2):
    pass

d = derived_class()
d.set_name()
print d.name
